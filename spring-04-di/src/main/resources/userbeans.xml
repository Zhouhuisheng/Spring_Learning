<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
        p命名空间注入，可以直接注入属性的值 property
        使用p命名空间需要依赖第三方约束 xmlns:p="http://www.springframework.org/schema/p"
     -->
    <bean name="user" class="com.kuang.pojo.User" p:name="小明" p:age="18"></bean>

    <!--
        c命名空间注入，可以通过构造器注入 construct-args
        使用c命名空间需要依赖第三方约束 xmlns:c="http://www.springframework.org/schema/c"
    -->
    <bean name="user2" class="com.kuang.pojo.User" c:name="小红" c:age="20"></bean>
</beans>
